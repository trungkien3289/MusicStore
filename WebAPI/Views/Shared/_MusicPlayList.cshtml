@using MusicStore.BussinessEntity
@model IList<SongEntity>
<div class="example">
    <div class="player">
        <div class="title"></div>
        <div class="artist"></div>
        <div class="cover"></div>
        <div class="controls">
            <div class="play"></div>
            <div class="pause"></div>
            <div class="rew"></div>
            <div class="fwd"></div>
        </div>
        <div class="volume"></div>
        <div class="tracker"></div>
    </div>
    <ul class="playlist">
        @for (int i = 0; i < Model.Count; i++)
        {
            <li audiourl="@Model[i].MediaUrl" cover="@Model[i].Thumbnail" artist="@Model[i].ArtistName" title="@Model[i].Title"><span>@(i + 1)</span>@Model[i].Title</li>
        }
    </ul>
</div>
